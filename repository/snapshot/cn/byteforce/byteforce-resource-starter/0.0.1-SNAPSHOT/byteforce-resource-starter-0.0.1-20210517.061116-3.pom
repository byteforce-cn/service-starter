    <dependencies>
        <dependency>
            <groupId>cn.byteforce</groupId>
            <artifactId>byteforce-core</artifactId>
        </dependency>
        <dependency>
            <groupId>org.springframework.cloud</groupId>
            <artifactId>spring-cloud-starter-openfeign</artifactId>
        </dependency>
        <dependency>
